# top-most EditorConfig file
root = true
# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
# Java Properties
[*.properties]
trim_trailing_whitespace = false
# XML files
[*.xml]
charset = utf-8
indent_size = 2
indent_style = space
[*.gradle]
charset = utf-8
indent_size = 2
indent_style = space
